        "module": "NodeNext",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "target": "es6",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "resolveJsonModule": true,
        "moduleResolution": "NodeNext",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": "."
    }
